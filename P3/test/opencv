cmake -D CMAKE_BUILD_TYPE=RELEASE \
    -D WITH_QUICKTIME=OFF -D WITH_QT=OFF -D WITH_GSTREAMER=OFF -D WITH_FFMPEG=OFF -D CMAKE_C_COMPILER=/usr/bin/clang -DCMAKE_CXX_COMPILER=/usr/bin/clang++ \
